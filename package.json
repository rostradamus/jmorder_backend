{
   "name": "temp",
   "version": "1.0.0",
   "description": "",
   "scripts": {
      "build": "yarn tsc",
      "tsc": "tsc --project tsconfig.compile.json",
      "tsc:w": "tsc --project tsconfig.json -w",
      "start": "nodemon",
      "start:prod": "cross-env NODE_ENV=production node dist/index.js",
      "test": "yarn test:lint && yarn test:coverage",
      "test:unit": "cross-env NODE_ENV=test mocha",
      "test:coverage": "cross-env NODE_ENV=test nyc mocha",
      "test:lint": "eslint src --ext .ts",
      "test:lint:fix": "eslint src --ext .ts --fix",
      "prettier": "prettier '{src,test}/**/*.ts' --write",
      "typeorm": "ts-node ./node_modules/.bin/typeorm"
   },
   "nodemonConfig": {
      "watch": [
         "src"
      ],
      "ext": "ts, html",
      "ignore": [
         "src/public",
         "node_modules/**/*"
      ],
      "exec": "ts-node -r tsconfig-paths/register ./src"
   },
   "dependencies": {
      "@tsed/ajv": "5.61.4",
      "@tsed/common": "5.61.4",
      "@tsed/core": "5.61.4",
      "@tsed/di": "5.61.4",
      "@tsed/exceptions": "^5.62.0",
      "@tsed/passport": "5.61.4",
      "@tsed/platform-express": "5.61.4",
      "@tsed/socketio": "5.61.4",
      "@tsed/swagger": "5.61.4",
      "@tsed/typeorm": "5.61.4",
      "@types/swagger-schema-official": "2.0.20",
      "ajv": "6.12.3",
      "body-parser": "1.19.0",
      "class-transformer": "^0.3.1",
      "compression": "1.7.4",
      "concurrently": "5.2.0",
      "cookie-parser": "1.4.5",
      "cors": "2.8.5",
      "cross-env": "7.0.2",
      "express": "4.17.1",
      "http-status-codes": "^1.4.0",
      "method-override": "3.0.0",
      "mysql": "^2.14.1",
      "passport": "0.4.1",
      "passport-local": "^1.0.0",
      "pg": "8.3.0",
      "reflect-metadata": "^0.1.10",
      "tsconfig-paths": "^3.9.0",
      "typeorm": "0.2.25",
      "typeorm-naming-strategies": "^1.1.0"
   },
   "devDependencies": {
      "@tsed/cli-plugin-eslint": "1.17.0",
      "@tsed/cli-plugin-mocha": "1.17.0",
      "@tsed/cli-plugin-passport": "1.17.0",
      "@tsed/cli-plugin-typeorm": "1.17.0",
      "@types/chai": "4.2.12",
      "@types/chai-as-promised": "7.1.3",
      "@types/compression": "1.7.0",
      "@types/cookie-parser": "1.4.2",
      "@types/cors": "2.8.6",
      "@types/express": "4.17.7",
      "@types/method-override": "0.0.31",
      "@types/mocha": "8.0.1",
      "@types/node": "^8.0.29",
      "@types/passport": "1.0.4",
      "@types/passport-local": "^1.0.33",
      "@types/sinon": "9.0.4",
      "@types/sinon-chai": "3.2.4",
      "@types/supertest": "2.0.10",
      "@typescript-eslint/eslint-plugin": "3.8.0",
      "@typescript-eslint/parser": "3.8.0",
      "chai": "4.2.0",
      "chai-as-promised": "7.1.1",
      "concurrently": "5.2.0",
      "eslint": "7.6.0",
      "eslint-config-prettier": "6.11.0",
      "eslint-plugin-prettier": "3.1.4",
      "husky": "4.2.5",
      "lint-staged": "10.2.11",
      "mocha": "8.1.1",
      "nodemon": "2.0.4",
      "nyc": "15.1.0",
      "prettier": "2.0.5",
      "sinon": "9.0.2",
      "sinon-chai": "3.5.0",
      "supertest": "4.0.2",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged",
         "post-commit": "git update-index --again"
      }
   }
}
